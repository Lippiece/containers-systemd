[Container]
ContainerName=synapse
Image=docker.io/matrixdotorg/synapse:latest

Volume=synapse_data:/data

PublishPort=4200:8008
PublishPort=4201:8009
PublishPort=4202:8448
Network=synapse.network

EnvironmentFile=synapse.env

AutoUpdate=registry

[Unit]
Requires=mas-db.service 
After=mas-db.service 
Wants=mas.service element-call-livekit.service element-call-jwt.service mautrix-telegram.service

[Service]
Restart=always

[Install]
WantedBy=default.target

#/* vim: set filetype=systemd: */
